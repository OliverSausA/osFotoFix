  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/osFotoFix/bin/Debug/net7.0/osFotoFix.dll",
      "args": [],
      "cwd": "${workspaceFolder}/osFotoFix",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "osFotoFix xUnit",
      "type": "coreclr",
      "request": "test",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/osFotoFix.Tests/bin/Debug/net7.0/osFotoFix.dll",
      "args": [],
      "cwd": "${workspaceFolder}/osFotoFix.Tests",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}